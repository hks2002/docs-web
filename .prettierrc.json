{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "singleQuote": true,
  "printWidth": 100,
  "importOrder": [
    "^#",
    "^@",
    "^src.*vue$",
    "^src"
  ],
  "importOrderGroupNamespaceSpecifiers": false,
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": false,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ]
}